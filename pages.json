{
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"disableScroll": true,
				"enablePullDownRefresh": false
			}
		},
		{
		    "path" : "pages/meeting/index",
		    "style": {
		        "navigationBarTitleText": "",
		        "enablePullDownRefresh": false,
				"disableScroll": true
		    }
		},
		{
            "path": "pages/outer/index",
            "style": {
                "enablePullDownRefresh": false,
				"disableScroll": true
            }
        }
